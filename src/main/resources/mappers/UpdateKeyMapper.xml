<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="com.platform.learning.reactbootproject.lecture.update_key.UpdateKeyMapper">
    <update id="updateIsAccess">
        update courses_access set isaccessgranted = '1' where user_id = #{userId} and course_id = #{courseId}
    </update>
</mapper>

